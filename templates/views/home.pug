doctype html
head
  // Required meta tags
  meta(charset='utf-8')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
  title NITRR genius
  // Bootstrap CSS
  link(rel='stylesheet', type='text/css', href='css/bootstrap.min.css')
  // Font
  link(rel='stylesheet', type='text/css', href='css/font-awesome.min.css')
  // Slicknav
  link(rel='stylesheet', type='text/css', href='css/slicknav.css')
  // Owl carousel
  link(rel='stylesheet', type='text/css', href='css/owl.carousel.css')
  link(rel='stylesheet', type='text/css', href='css/owl.theme.css')
  // Animate
  link(rel='stylesheet', type='text/css', href='css/animate.css')
  // Main Style
  link(rel='stylesheet', type='text/css', href='css/main.css')
  // Extras Style
  link(rel='stylesheet', type='text/css', href='css/extras.css')
  // Responsive Style
  link(rel='stylesheet', type='text/css', href='css/responsive.css')
// Header Area wrapper Starts
header#header-wrap
  // Navbar Start
  nav.navbar.navbar-expand-lg.fixed-top.scrolling-navbar.indigo
    .container
      // Brand and toggle get grouped for better mobile display
      .navbar-header
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='#main-navbar', aria-controls='main-navbar', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
          span.icon-menu
          span.icon-menu
          span.icon-menu
        a.navbar-brand(href='/')
          span(style='color:white;') NITRR Genius


         
      #main-navbar.collapse.navbar-collapse
        ul.navbar-nav.mr-auto.w-100.justify-content-end.clearfix
          li.nav-item.active
            a.nav-link(href='')
          li.nav-item
            a.nav-link(href='/blog')
              | Doubts
          li.nav-item
            a.nav-link(href='/notes')
              | Notes
          li.nav-item
            a.nav-link(href='/contact')
              | Enquiry us 
          li.nav-item
            a.nav-link(href='/map')
              | Maps       
          li.nav-item
            a.nav-link(href='/CPI')
              | CPI
          li.nav-item
            a.nav-link(href='/previous')
              | Previous
          li.nav-item
            a.nav-link(href='/quiz')
              | Quiz              
          if user
              li.nav-item: a.nav-link(href='/keystone') Database
              li.nav-item: a.nav-link(href='/keystone/signout') Sign out
          else
              li.nav-item: a.nav-link(href='/keystone/signin') Sign in              

    // Mobile Menu Start
    ul.mobile-menu.navbar-nav
      li
        a.page-scroll(href='#sliders')
          | Home
      li
        a.page-scroll(href='/blog')
          | Doubts
      li
        a.page-scroll(href='/gallery')
          | Notes
      li
        a.page-scroll(href='#contact')
          | Enquiry
      li
        a.page-scroll(href='/CPI')
          | CPI
      li
        a.page-scroll(href='/previous')
          | Previus year
      li
        a.page-scroll(href='quiz')
          | Quiz
      if user
          li: a.page-scroll(href='/keystone') Database
          li: a.page-scroll(href='/keystone/signout') Sign out
      else
          li: a.page-scroll(href='/keystone/signin') Sign in      
      //- li
      //-   a.page-scroll(href='#contact')
      //-     | Contact
      // Mobile Menu End
      // Navbar End
      // sliders
 nav.navbar.navbar-default.navbar-fixed-top
       .container
          .navbar-header
               button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-collapse', aria-expanded='false')
                   span.sr-only Toggle navigation
                  span.icon-bar
                  span.icon-bar
                   span.icon-bar
                   a.navbar-brand(href='/') Nitrr Genious
         .collapse.navbar-collapse#navbar-collapse
                    ul.nav.navbar-nav.navbar-left
                      each link in navLinks
                       li(class=(section === link.key ? 'active' : null)): a(href=link.href)= link.label
                   ul.nav.navbar-nav.navbar-right
                      if user
                        li: a(href='/keystone') Database
                           li: a(href='/keystone/signout') Sign Out
                      else
                         li: a(href='/keystone/signin') Sign In
  #sliders
    .full-width
      // light slider
      #light-slider.carousel.slide
        #carousel-area
          #carousel-slider.carousel.slide(data-ride='carousel')
            ol.carousel-indicators
              li.active(data-target='#carousel-slider', data-slide-to='0')
            .carousel-inner(role='listbox')
              .carousel-item.active
                img(src='img/slider/98.jpeg', alt='')
                #menutitle.carousel-caption
                  h3.slide-title.animated.fadeInDown
                    span 
                    | NITRR Genius
                    br
                    span Education Vending Platform
                  h5.slide-text.animated.fadeIn
                    | One platform of all education of NITRR
                    br
                    |  notes, assignments, previous year&apos;s, doubts, CPI enhancement and much more

                  if user
                    a#baba.oneaa.btn.btn-lg.btn-common.animated.fadeInUp(href='/keystone/signout') Sign Out
                    a#tsts.twoaa.btn.btn-lg.btn-common.animated.fadeInUp(href='/keystone/galleries?new') Upload Notes
                  else
                    a#ava.oneaa.btn.btn-lg.btn-common.animated.fadeInUp(href='/keystone/signin') Sign In
                    a#bab.twoaa.btn.btn-lg.btn-common.animated.fadeInUp(href='/keystone/galleries?new') Upload Notes
        .search
            <gcse:search></gcse:search>              
  // End sliders
// Header Area wrapper End
// About Section Start
#about.section-padding
  .container
    .row
      .col-lg-3.col-md-6.col-xs-12
        .about.block.text-center
          img(src='img/1.png', alt='')
          h5.nichewaala
            a(href='/keystone') Student&apos;s library
          p.nichekar
            | A large digital collection of
            br
            |  notes, doubts, articles, previous year&apos;s
      .col-lg-3.col-md-6.col-xs-12
        .about.block.text-center
          img(src='img/2.png', alt='')
          h5.nichewaala
            a(href='/keystone') Student&apos;s Panel
          p.nichekar
            | Personalized Dashboard 
            br
            |  My Doubts 
            br
            |  My Tests Scores
      .col-lg-3.col-md-6.col-xs-12
        .about.block.text-center
          img(src='img/3.png', alt='')
          h5.nichewaala
            a(href='/blog') Open Doubts 
          p.nichekar
            | Ask any doubt in platform
            br
            | It&apos;s all sorted 
            br
            |               open to all
      .col-lg-3.col-md-6.col-xs-12
        .about.block.text-center
          img(src='img/4.png', alt='')
          h5.nichewaala
            a(href='/cpi') CPI enhancement
          p.nichekar
            | Complex algorithm and analysis  
            br
            |               to tell us how to improve our performance it
// About Section End
// Services Section Start
section#services.section-padding
  .container
    .row
      .col-md-12
        h2.section-title.wow.fadeInDown.animated(data-wow-delay='0.3s') Our Features
    .row(style=' font-size: 25px;')
      // Start Service Icon 1
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-book
          .service-content
            h4
              a(href='/previous') Previous Year&apos;s
            p
              | Cummulative collection of all previous year question&apos;s
      // End Service Icon 1
      // Start Service Icon 2
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-cubes
          .service-content
            h4
              a(href='/notes') Notes collection
            p
              | Open collection of notes 
              br
              | sorted by subjects and Topics
      // End Service Icon 2
      // Start Service Icon 3
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-search
          .service-content
            h4
              a(href='/') Google custom search 
            p
              | Search any query
              | get all the articles, notes, doubts and questions which mentions it
      // End Service Icon 3
      // Start Service Icon 4
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-sticky-note
          .service-content
            h4
              a(href='/keystone/post-categories') Topics
            p
              | Notes and doubts 
              br
              | are sorted by the topics and content
      // End Service Icon 4
      // Start Service Icon 5
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-bell-o
          .service-content
            h4
              a(href='https://docs.google.com/spreadsheets/d/1np_Yl_ZY-dDDZSpWg0_ZkYmRuhLFd3CEp_USS9qmkNE/edit?usp=sharing') Schedule 
            p
              | Reminder for assignments,
              br
              |  exams and submissions
      // End Service Icon 5
      // Start Service Icon 6
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-graduation-cap
          .service-content
            h4
              a(href='quiz') Test Skills
            p
              | Attempt specially designed mock tests
              | and improve your skills
              | and compare with other&apos;s in class
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-map-marker
          .service-content
            h4
              a(href='/map') Maps 
            p
              | Needy college campus maps
              | with all floors and labelled classes
      .col-md-6.col-lg-4.col-xs-12
        .service-box
          .service-icon
            i.fa.fa-bell
          .service-content
            h4
              a(href='/Notification') Notifications 
            p
              | Get notified and stay aware
              | for all events and competions being organised              
      // End Service Icon 6
// Services Section End
// Footer Section End
// Go to Top Link
a.back-to-top(href='#')
  i.fa.fa-arrow-up
